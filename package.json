{
  "name": "junglebook",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel & npm run test:ui",
    "lint": "turbo run lint",
    "build": "turbo run build",
    "test": "vites trun ",
    "test:ci": "vitest run --coverage",
    "test:watch": "vitest watch"
    "test:ui": "vitest --ui --open",
    "ci": "turbo run ci"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^27.0.0",
    "turbo": "^2.2.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@10.8.2"
}
